<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jgeun.study.hansung_mbti">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hansung_Mbti">
        <activity
            android:screenOrientation="portrait"
            android:name=".result.ResultActivity" />
        <activity android:screenOrientation="portrait" android:name=".question.QuestionActivity">

        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".loading.LoadingActivity" />
        <activity android:screenOrientation="portrait" android:name=".splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:screenOrientation="portrait" android:name=".MainActivity" />
    </application>

</manifest>